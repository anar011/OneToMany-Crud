
@{
    ViewData["Title"] = "Create";
}

<div class="container">
    <form>
  <div class="form-row mx-5 " >
    <div class="form-group col-md-6">
      <label for="inputEmail4">Event Name</label>
      <input type="email" class="form-control" id="inputEmail4" placeholder="Write News Name">
    </div>
    <div class="form-group col-md-6">
      <label for="inputPassword4">Event Time</label>
      <input type="password" class="form-control" id="inputPassword4" placeholder="Write Name">
    </div>
  </div>

  <div class="form-group">
    <label for="inputAddress2"></label>
    <input type="text" class="form-control" id="inputAddress2" >
  </div>
  <div class="form-row">
    <div class="form-group col-md-6">
      <label for="inputCity">Event Location</label>
      <input type="text" class="form-control" id="inputCity">
    </div>
    <div class="form-group col-md-4">
      <label for="inputState">State</label>
      <select id="inputState" class="form-control">
        <option selected>Choose...</option>
        <option>...</option>
      </select>
    </div>
    <div class="form-group col-md-2">
      <label for="inputZip">Zip</label>
      <input type="text" class="form-control" id="inputZip">
    </div>
  </div>


</form>

<div class="my-3">
     <button type="submit" class="btn btn-primary mr-2" >Submit</button>
 <a class="btn btn-light" asp-action="Index">Cancel</a>
</div>




</div>